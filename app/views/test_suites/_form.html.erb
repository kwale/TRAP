<%= form_for(@test_suite) do |f| %>
  <% if @test_suite.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@test_suite.errors.count, "error") %> prohibited this test_suite from being saved:</h2>

      <ul>
      <% @test_suite.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :test_suite %><br />
    <%= f.text_field :test_suite %>
  </div>
  <div class="field">
    <%= f.label :failures %><br />
    <%= f.number_field :failures %>
  </div>
  <div class="field">
    <%= f.label :passed %><br />
    <%= f.number_field :passed %>
  </div>
  <div class="field">
    <%= f.label :Nr_tests %><br />
    <%= f.number_field :Nr_tests %>
  </div>
  <div class="field">
    <%= f.label :timestamp %><br />
    <%= f.text_field :timestamp %>
  </div>
  <div class="field">
    <%= f.label :execution_time %><br />
    <%= f.datetime_select :execution_time %>
  </div>
  <div class="field">
    <%= f.label :tag %><br />
    <%= f.text_field :tag %>
  </div>
  <div class="field">
    <%= f.label :release %><br />
    <%= f.text_field :release %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
